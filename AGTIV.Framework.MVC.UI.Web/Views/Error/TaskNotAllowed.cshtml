@using MMCGamuda.SPS.Framework.Constants

<div class="content-wrapper">
    <section class="content-header">
        <h1>
            Invalid Task
        </h1>
    </section>
    <section class="content col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="error-page">
            <div class="error-content">
                <h3><i class="fa fa-warning text-yellow"></i> Oops! @ConstantHelper.Error.User.NotMyTask .</h3>
                <p>
                    You are not authorized to act on this task.
                    Meanwhile, you may <a href="@Url.Action("Index", "Request")">return to home</a>.
                </p>
            </div>
        </div>
    </section>
</div>
